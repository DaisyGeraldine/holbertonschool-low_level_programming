CC = gcc #compiler
SRC = main.c school.c #files .c
OBJ = $(SRC:.c=.o) #file .o
NAME = school
#RM = rm -f
CFLAGS = -Wall -Werror -Wextra -pedantic

all : m.h $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

.PHONY : clean oclean fclean all

clean :
	$(RM) *~ $(NAME)

oclean :
	$(RM) $(OBJ)

fclean : clean oclean

re : oclean all
