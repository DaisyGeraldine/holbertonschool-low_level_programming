#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/**
 * main - Entry point
 *
 * Description: 'Print last digit of a number random'
 * Betty style doc for function main goes there
 * Return: Always 0 (Success)
 */
int main(void)
{
	int n;
	int d;
	srand(time(0));
	n = rand() - RAND_MAX / 2;
	d = n % 10;
	printf("Last digit of %d is %d\n", n,d);
	return (0);
}
